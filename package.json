{
  "name": "@autolotto/bunnyhop",
  "version": "2.1.0",
  "main": "build/index.js",
  "license": "MIT",
  "repository": "https://github.com/autolotto/bunnyhop",
  "scripts": {
    "release": "yarn build:clean && yarn build && np",
    "build": "babel src -s -d build --ignore src/example,src/**/*.test.js",
    "build:clean": "rm -rf build",
    "dev": "watch 'npm run build' src",
    "test": "ava -v -T 5000",
    "test:watch": "npm run test -- -w"
  },
  "babel": {
    "presets": [
      [
        "env",
        {
          "targets": {
            "node": "6.5.0"
          }
        }
      ]
    ]
  },
  "ava": {
    "require": "babel-register",
    "babel": "inherit"
  },
  "dependencies": {
    "amqplib": "^0.5.1",
    "death": "^1.1.0",
    "debug": "^2.6.8",
    "lodash": "^4.17.4",
    "uuid": "^3.0.1"
  },
  "devDependencies": {
    "ava": "^0.19.1",
    "babel-cli": "^6.24.1",
    "babel-preset-env": "^1.5.2",
    "babel-register": "^6.24.1",
    "bluebird": "^3.5.0",
    "np": "^2.15.0",
    "testdouble": "^3.0.0",
    "watch": "^1.0.2"
  }
}
