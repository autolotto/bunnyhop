{
  "name": "@autolotto/bunnyhop",
  "version": "2.0.0",
  "main": "build/index.js",
  "license": "MIT",
  "scripts": {
    "release": "yarn build:clean && yarn build && np",
    "build": "babel src -s -d build --ignore src/example",
    "build:clean": "rm -rf build",
    "dev": "watch 'npm run build' src"
  },
  "babel": {
    "presets": [
      [
        "env",
        {
          "targets": {
            "node": "6.5.0"
          }
        }
      ]
    ]
  },
  "dependencies": {
    "amqplib": "^0.5.1",
    "death": "^1.1.0",
    "debug": "^2.6.8",
    "lodash": "^4.17.4",
    "uuid": "^3.0.1"
  },
  "devDependencies": {
    "babel-cli": "^6.24.1",
    "babel-preset-env": "^1.5.2",
    "np": "^2.15.0",
    "watch": "^1.0.2"
  }
}
